<!DOCTYPE html>
<html lang="en">
<head>
    <title>Posts</title>
</head>
<body>
    <h1>Posts</h1>
    <ul>
        <% posts.forEach(post => { %>
            <li>
                <%= post.user_id ? post.user_id.username : "Unknown User" %> - 
                <%= post.player_id ? post.player_id.name : "Unknown Player" %>
                <br>
                <%= post.content %>
            </li>
        <% }) %>
    </ul>
    
</body>
</html>